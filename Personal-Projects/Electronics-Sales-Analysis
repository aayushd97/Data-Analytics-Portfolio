    "## Combining all 12 months of Sales Data into one .csv file "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "id": "fd25ff5c-d0ff-4faf-be20-14f974920443",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Concatenating the 12 different csv files\n",
    "\n",
    "df=pd.read_csv(\"Sales-File/Sales_April_2019.csv\")\n",
    "\n",
    "files = [file for file in os.listdir('./Sales-File')]\n",
    "\n",
    "yearly_data = pd.DataFrame()\n",
    "\n",
    "for file in files:\n",
    "    df=pd.read_csv(\"Sales-File/\"+file)\n",
    "    yearly_data = pd.concat([yearly_data, df])\n",
    "\n",
    "yearly_data.to_csv(\"all_data.csv\", index=False)"
   ]
  },
