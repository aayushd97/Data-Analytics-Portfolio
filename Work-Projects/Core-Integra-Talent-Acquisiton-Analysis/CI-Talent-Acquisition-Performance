  {
   "cell_type": "code",
   "execution_count": 137,
   "id": "12b7c801-dd1d-4da7-8c9c-ffef15dadb17",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Reference Category\n",
       "Naukri                  5728\n",
       "Job Hai                  458\n",
       "Skill Connect            340\n",
       "Other                    335\n",
       "Surender Diwedi          243\n",
       "Candidate Reference      134\n",
       "Local Reference          107\n",
       "Captain Tobby Joseph      88\n",
       "Ex-Servicemen             86\n",
       "Client Referance          71\n",
